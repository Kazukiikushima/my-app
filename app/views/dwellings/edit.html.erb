<%= form_for(@dwelling) do |f| %>
  <% if @dwelling.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@dwelling.errors.count, "error") %> prohibited this estimate from being saved:</h2>

      <ul>
      <% @dwelling.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :comment %><br>
    <%= f.text_field :comment %>
